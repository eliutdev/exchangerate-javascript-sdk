{
  "name": "exchangerate-javascript-sdk",
  "version": "2.1.1",
  "type": "module",
  "description": "Unofficial JavaScript SDK for Exchange rates API",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "typedoc": "^0.21.5"
  },
  "scripts": {
    "docs": "npx typedoc src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "rm -fr dist/* && tsc -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "currency-exchange-rates",
    "javascript-sdk",
    "exchangerate",
    "exchangerate-api"
  ],
  "author": "Eliut Gonz√°lez",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.4.13",
    "fs-extra": "^10.0.0",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reliut-g/exchangerate-javascript-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/reliut-g/exchangerate-javascript-sdk/issues"
  },
  "homepage": "https://github.com/reliut-g/exchangerate-javascript-sdk#readme"
}
